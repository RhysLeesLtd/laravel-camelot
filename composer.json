{
  "name": "rhysleesltd/laravel-camelot",
  "description": "PHP Wrapper library for interfacing with the Camelot PDF table extraction library built in Python",
  "type": "library",
  "authors": [
    {
      "name": "Connor Forsyth",
      "email": "connor@randomstate.co.uk"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3|^8.4|^8.5",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "symfony/process": "^5.0|^6.0|^7.0|^8.0",
    "spatie/temporary-directory": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "RhysLeesLtd\\Camelot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RhysLeesLtd\\Camelot\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "league/csv": "^9.8"
  },
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "RhysLeesLtd\\Camelot\\CamelotServiceProvider"
      ]
    }
  }
}
